pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat "mvn -Dmaven.test.failure.ignore-true clean package"
            }

        }
        stage('Test') {
            steps {
                echo "Test Stage"
            }
            
        }
        stage('Package') {
            steps {
                echo "Package Stage"
            }
        }
        stage('Publish') {
            steps {
                echo "Publish Stage"
            }
        }     
    }
    
      post {
        success {
        junit '**/target/surefire-reports/TEST-*.xml'
        archiveArtifacts 'target/*.jar'
      }
    }
}
